    <div class="container my-4 mx-auto" id="forms">




        <div class="form-group animated fadeIn" id="search_form">
            <div class="form-row">
                <div class="form-group col-md-8">

                    <label for="numberSert">{{{__ "Номер  сертификата"}}}</label>
                    <input type="text" size="3" value="0" id="numberSert" name="numberSert" maxlength="9"
