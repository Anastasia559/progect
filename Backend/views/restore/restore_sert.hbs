                <div class="form-group col-md-3">
                    <label for="ort_physics"> {{{__ "Физика"}}}</label>
                    <input type="number" size="2" value="0" id="ort_physics" name="ort_physics" min="0" max="150"
                        class="form-control">
                </div>
            </div>
            <div class="form-row">



                <div class="form-group col-md-3">
                    <label for="ort_english"> {{{__ "Английский язык"}}}</label>
                    <input type="number" size="2" value="0" id="ort_english" name="ort_english" min="0" max="150"
                        class="form-control">

                </div>
                <div class="form-group col-md-3">

                    <label for="ort_math"> {{{__ "Математика"}}}</label>
                    <input type="number" size="2" value="0" id="ort_math" name="ort_math" min="0" max="150"
