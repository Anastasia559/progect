                <div class="form-group col-md-3">
                    <label for="ort_physicsn"> {{{__ "Физика"}}}</label>
                    <input type="number" size="2" value="0" id="ort_physicsn" name="ort_physicsn" min="0" max="150"
                        class="form-control">
                </div>
            </div>
            <div class="form-row">



                <div class="form-group col-md-3">
                    <label for="ort_englishn"> {{{__ "Английский язык"}}}</label>
                    <input type="number" size="2" value="0" id="ort_englishn" name="ort_englishn" min="0" max="150"
