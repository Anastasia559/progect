<center> 
<div class="container">

    <form class="well form-horizontal" action=" " method="post"  id="contact_form">
<fieldset>

<!-- Form Name -->
<legend>Контактная информация</legend>

<!-- Text input-->
{{#each obj}}
{{#each this}}
<div class="form-group">
    {{#each this}}
  <label class="col-md-4 control-label">ФИО</label>  
  <div class="col-md-4 inputGroupContainer">
  <div class="input-group">
  <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
  <input  name="first_name" placeholder="First Name" class="form-control"  type="text" value="{{fio_users_university}}">
    </div>
  </div>
</div>



<!-- Text input-->
       <div class="form-group">
  <label class="col-md-4 control-label">E-Mail</label>  
    <div class="col-md-4 inputGroupContainer">
    <div class="input-group">
        <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
  <input name="email" placeholder="E-Mail Address" class="form-control"  type="text" value="{{email}}">
    </div>
  </div>
</div>


<!-- Text input-->
       
<div class="form-group">
  <label class="col-md-4 control-label">Мобильный Телефон</label>  
    <div class="col-md-4 inputGroupContainer">
    <div class="input-group">
        <span class="input-group-addon"><i class="glyphicon glyphicon-earphone"></i></span>
  <input name="phone" placeholder="(845)555-1212" class="form-control" type="text" value="{{mobile_phone}}">
    </div>
  </div>
</div>

<!-- Text input-->
       
<div class="form-group">
  <label class="col-md-4 control-label">Официальный номер</label>  
    <div class="col-md-4 inputGroupContainer">
    <div class="input-group">
        <span class="input-group-addon"><i class="glyphicon glyphicon-earphone"></i></span>
  <input name="phone" placeholder="(845)555-1212" class="form-control" type="text" value="{{official_phone}}">
    </div>
  </div>
</div>

<div class="social_button">
   {{#if watsup}}<button type="button" class="btn btn-success"><img class="mr-2" style="height: 30px;" src="/img/watsup.png" alt="image"> <font size="4">{{watsup}} </font></button> {{/if}}
        {{#if telegram}}<button type="button" class="btn btn-info"><img class="mr-2" style="height: 30px;" src="/img/telegram.png" alt="image"><font size="4">{{telegram}} </font></button>{{/if}}
        {{#if email}}<button type="button" class="btn btn-link"><font size="4">{{email}} </font></button> {{/if}}

        {{#if instagram}}<a target="_blank" href={{instagram}}><img class="mr-2" style="height: 40px;" src="/img/instagram.png" alt="image"></a>{{/if}}
        {{#if facebook}} <a target="_blank" href={{facebook}}><img class="mr-2" style="height: 40px;" src="/img/facebook.png" alt="image"></a>{{/if}}
        {{#if faculty_ulr}} <a target="_blank" class="btn btn-block btn-social btn-twitter" href={{faculty_ulr}}><img class="mr-2" style="height: 43px;" src="/img/www.png" alt="image"></a>{{/if}}
   
</div>

</fieldset>
</form>
</div>
